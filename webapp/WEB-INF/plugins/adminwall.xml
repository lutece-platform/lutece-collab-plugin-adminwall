<?xml version="1.0" encoding="ISO-8859-1"?>
<plug-in>
    <name>adminwall</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.0</version>
    <documentation></documentation>
    <installation></installation>
    <changes></changes>
    <user-guide></user-guide>
    <description>adminwall.plugin.description</description>
    <provider>adminwall.plugin.provider</provider>
    <provider-url>http://fr.lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright</copyright>
    <db-pool-required>1</db-pool-required>
    
     <!-- Feuilles de style CSS spécifiques éventuelles -->
    <css-stylesheets>
      
    </css-stylesheets>
    
    <!-- Feuilles de style CSS admin spécifiques éventuelles -->
    <admin-css-stylesheets>
        <admin-css-stylesheet>css/admin/AdminLTEv2.css</admin-css-stylesheet>
    </admin-css-stylesheets>
    
    
    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>ADMINWALL_ACCESS</feature-id>
            <feature-title>adminwall.adminFeature.AdminWall.name</feature-title>
            <feature-description>adminwall.adminFeature.AdminWall.description</feature-description>
            <feature-level>3</feature-level>
            <feature-url>jsp/admin/plugins/adminwall/ManageWall.jsp</feature-url>
            <feature-icon-url></feature-icon-url>
      </admin-feature>
    </admin-features>    
  	<!-- XPages parameters -->
    <applications>
    </applications>
	<!-- Portlet parameters -->
    <portlets>
   </portlets>
   
   <!-- Dashboard components -->
    <dashboard-components>
        <dashboard-component>
            <dashboard-component-name>ADMINWALL</dashboard-component-name>
            <dashboard-component-class>fr.paris.lutece.plugins.adminwall.web.AdminWallDashboardComponent</dashboard-component-class>
            <dashboard-feature-right>ADMINWALL_ACCESS</dashboard-feature-right>
            <dashboard-zone>1</dashboard-zone>
            <dashboard-zone-order>4</dashboard-zone-order>
        </dashboard-component>
    </dashboard-components>
    
</plug-in>